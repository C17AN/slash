{
  "name": "@tossteam/impression-area",
  "version": "17.194.6",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./testing": "./src/testing/index.ts"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "esm/index.js",
    "import": "esm/index.js",
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "import": "./esm/index.js",
        "types": "./dist/index.d.ts"
      },
      "./package.json": "./package.json",
      "./testing": {
        "require": "./dist/testing/index.js",
        "import": "./esm/testing/index.js",
        "types": "./dist/testing/index.d.ts"
      }
    }
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "concurrently \"tsc -p tsconfig.json\" \"tsc -p tsconfig.esm.json\"",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/runtime": "^7.13.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/lodash.debounce": "^4",
    "@types/node": "^14.14.41",
    "@types/react-dom": "^18",
    "@types/testing-library__jest-dom": "^5",
    "concurrently": "^4.1.0",
    "jest": "^29.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "4.8.3"
  },
  "sideEffects": false,
  "dependencies": {
    "@tossteam/use-combined-refs": "workspace:^17.194.6",
    "@tossteam/use-preserved-callback": "workspace:^17.194.6",
    "@tossteam/use-ref-effect": "workspace:^17.194.6",
    "@tossteam/utils": "workspace:^17.194.4",
    "@types/react": "^17.0.3",
    "lodash.debounce": "^4.0.8"
  },
  "peerDependencies": {
    "react": "*"
  }
}
